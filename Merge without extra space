void merge(long long a1[], long long a2[], int n, int m) 
        { 
            for(int i=0;i<n;i++){
            if(a1[i]>a2[0]){
           
            a1[i]=a1[i]^a2[0];
            a2[0]=a1[i]^a2[0];
            a1[i]=a1[i]^a2[0];
            
        }
        sort(a2,a2+m);
        
        // sort(a2,a2+m);
       }
    //   sort(a1,a1+n);
        } 
